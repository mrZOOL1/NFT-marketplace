generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Cards {
  id     String @id
  userid String
  title  String
  price  String
  image  String
  owner  String
}

model Carts {
  userid String
  cardid String
  cartid String @id
}

model Likes {
  userid String
  cardid String
  likeid String @id
}
