generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Cards {
  id     String @id
  userid String
  title  String
  price  String
  image  String
  owner  String
}

model Carts {
  userid String @id
  cardid String
}

model Likes {
  userid String @id
  cardid String
}

model Users {
  id       String @id
  email    String
  password String
  name     String
  image    String
}
